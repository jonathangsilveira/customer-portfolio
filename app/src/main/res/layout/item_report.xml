<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="?attr/listPreferredItemHeight">

    <TextView
        android:id="@+id/item_report_name"
        android:layout_width="250dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        />

    <TextView
        android:id="@+id/item_report_state"
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_name" />

    <TextView
        android:id="@+id/item_report_document"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_state"/>

    <TextView
        android:id="@+id/item_report_id"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_document"/>

    <TextView
        android:id="@+id/item_report_birthdate"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_id"/>

    <TextView
        android:id="@+id/item_report_telephone"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_birthdate"/>

    <TextView
        android:id="@+id/item_report_registered_at"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:lines="1"
        app:layout_constraintStart_toEndOf="@+id/item_report_telephone" />

</android.support.constraint.ConstraintLayout>