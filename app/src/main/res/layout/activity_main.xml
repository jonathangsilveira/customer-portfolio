<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/activity_main_manter_cliente"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/manter_cliente"
        app:layout_constraintBottom_toBottomOf="@+id/activity_main_relatorio"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/activity_main_relatorio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/relatorio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/activity_main_manter_cliente" />

    <Button
        android:id="@+id/activity_main_configuracoes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/configuracoes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/activity_main_relatorio" />

</android.support.constraint.ConstraintLayout>